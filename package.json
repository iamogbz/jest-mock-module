{
    "author": "iamogbz",
    "bugs": {
        "url": "https://github.com/iamogbz/jest-mock-module/issues"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "jest-mock-props": "^1.7.5"
    },
    "description": "Project description",
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/preset-typescript": "^7.10.4",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@commitlint/travis-cli": "^11.0.0",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "acorn": "^8.0.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "canvas": "^2.6.1",
        "commitizen": "^4.2.1",
        "coveralls": "^3.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "jest": "^25.5.4",
        "lint-staged": "^10.4.0",
        "prettier": "^2.1.2",
        "prettier-eslint": "^11.0.0",
        "rxjs": "^6.6.3",
        "semantic-release": "^17.1.2",
        "stylelint": "^13.7.1",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.10.0",
        "ts-jest": "^25.5.1",
        "ts-node": "^9.0.0",
        "typescript": "^3.9.7"
    },
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "files": [
        "dist",
        "typings"
    ],
    "homepage": "https://github.com/iamogbz/jest-mock-module#readme",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "jest": {
        "coverageDirectory": "./artifacts/coverage",
        "moduleDirectories": [
            ".",
            "./node_modules"
        ],
        "preset": "ts-jest",
        "testPathIgnorePatterns": [
            "./artifacts/",
            "./dist/",
            "./node_modules/"
        ]
    },
    "keywords": [],
    "license": "Unlicense",
    "lint-staged": {
        "*.{js,ts}": [
            "npm run lint",
            "npm test -- --bail --findRelatedTests"
        ]
    },
    "main": "dist/index.js",
    "name": "jest-mock-module",
    "release": {
        "dryRun": true,
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            ["@semantic-release/npm", { "pkgRoot": "./dist"}],
            "@semantic-release/github"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/iamogbz/jest-mock-module"
    },
    "scripts": {
        "build": "tsc --project tsconfig.prod.json",
        "build-watch": "tsc --watch",
        "commit": "git-cz",
        "coveralls": "cat ./artifacts/coverage/lcov.info | coveralls",
        "lint": "eslint . --ext .js,.ts",
        "package": "rm -rf ./dist && npm run build && cp -t ./dist package.json LICENSE",
        "release": "semantic-release",
        "test": "jest",
        "typecheck": "tsc --noEmit"
    },
    "types": "dist/index.d.ts",
    "version": "0.0.1"
}
